import requests

OkHttpClient client = new OkHttpClient().newBuilder()
 .build();
Request request = new Request.Builder()
 .url("https://api.geoapify.com/v2/places?categories=catering.restaurant&filter=place:51d78dcdee167c52c059a5d22785e3544440f00101f90121af020000000000c0020a92031043697479206f66204e657720596f726b&lang=en&limit=20&apiKey=YOUR_API_KEY")
 .method("GET", null)
 .build();
Response response = client.newCall(request).execute();